<table class="table table-condensed">
  <thead>
    <tr>
        <th>Name</th>
        <th>Date</th>
        <th></th>
    </tr>
  </thead>
  <tbody>
    <% @sold.each do |sale| %>
      <tr>
      <td><%= link_to sale.medication.name, pickup_path(sale.uuid) %></td>
      <td><%= time_ago_in_words(sale.created_at) %> ago </td>
      </tr>
    <% end %>
  </tbody>
</table>
